Func = Library:Tab("实用工具")
LocalPlayer = Library:Tab("玩家")
PlaceInfo = Library:Tab("作品信息")
ScriptInfo = Library:Tab("脚本作者信息")
---------------------------------------
function loopprintwalkspeed()
    while _G.loopprintwalkspeed == true do
        print(game.Players.LocalPlayer.Character.Humanoid.WalkSpeed)
        wait()
    end
end
function loopprintjumppower()
    while _G.loopprintjumppower == true do
        print(game.Players.LocalPlayer.Character.Humanoid.JumpPower)
        wait()
    end
end
function loopprintgravity()
    while _G.loopprintgravity == true do
        print(workspace.Gravity)
        wait()
    end
end
function seewalkspeed()
    while _G.seewalkspeed == true do
        WalkSpeedLabel.Text = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
        WalkSpeedLabel.Visible = true
        wait()
    end
end
function seejp()
    while _G.seejp == true do
        JPVal.Text = game.Players.LocalPlayer.Character.Humanoid.JumpPower
        JPVal.Visible = true
        wait()
    end
end
function seegr()
    while _G.seegr == true do
        GRVal.Text = workspace.Gravity
        GRVal.Visible = true
        wait()
    end
end
