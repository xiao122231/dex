LocalPlayer:Button("打印WalkSpeed",function()
    print(game.Players.LocalPlayer.Character.Humanoid.WalkSpeed)
end)
LocalPlayer:Toggle("持续打印WalkSpeed",false,function(value)
    _G.loopprintwalkspeed = value
    loopprintwalkspeed()
end)
LocalPlayer:Toggle("速度仪表盘",false,function(value)
    _G.seewalkspeed = value
    seewalkspeed()
    while _G.seewalkspeed == false do
        WalkSpeedLabel.Visible = false
        wait()
    end
end)
LocalPlayer:Button("打印重力",function()
    print(game.Gravity)
end)
LocalPlayer:Toggle("持续打印JumpPower",false,function(value)
    _G.loopprintjumppower = value
    loopprintjumppower()
end)
LocalPlayer:Toggle("跳跃高度仪表盘",false,function(value)
    _G.seejp = value
    seejp()
    while _G.seejp == false do
        JPVal.Visible = false
        wait()
    end
end)
LocalPlayer:Button("打印Gravity",function()
    print(game.Gravity)
end)
LocalPlayer:Toggle("持续打印Gravity",false,function(value)
    _G.loopprintgravity = value
    loopprintgravity()
end)
LocalPlayer:Toggle("重力仪表盘",false,function(value)
    _G.seegr = value
    seegr()
    while _G.seegr == false do
        GRVal.Visible = false
        wait()
    end
end)
